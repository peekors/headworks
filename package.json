{
  "name": "headworks",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/anisimovv/headworks",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react": "*",
    "@mate-academy/stylelint-config": "*",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "stylelint": "^10.0.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --ext .jsx,.js ./src/",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.css": [
      "stylelint",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
